"*":
  "atom-beautify":
    general:
      _analyticsUserId: "795eef1c-c9a2-45a0-8c4a-e32d8da410cf"
    html:
      end_with_newline: true
      indent_size: 2
      wrap_attributes: "force-aligned"
      wrap_line_length: 70
    js:
      end_with_newline: true
      indent_size: 4
    jsx:
      end_with_newline: true
      indent_size: 4
    typescript:
      break_chained_methods: true
      end_with_newline: true
      indent_size: 2
  "atom-ide-ui":
    hyperclick:
      darwinTriggerKeys: "altKey"
  "atom-shell-commands":
    commands: [
      {
        command: "echo moooooo"
        name: "SmartBuild"
        options:
          cwd: "{FileDir}"
          keymap: "cmd-shift-b"
      }
    ]
  autosave:
    enabled: true
  core:
    audioBeep: false
    closeEmptyWindows: false
    disabledPackages: [
      "jshint"
      "linter"
      "markdown-preview"
      "markdown-preview-plus"
    ]
    followSymlinks: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "README.md"
      "CHANGELOG.md"
      "package-lock.json"
    ]
    packagesWithKeymapsDisabled: [
      "emmet"
    ]
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "no"
  editor:
    fontSize: 16
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabType: "soft"
  "exception-reporting":
    userId: "c8be361d-65b0-e083-6643-808e2d26edb2"
  fonts: {}
  "git-blame":
    columnWidth: 231
    dateFormatString: "MMM-DD"
    showHash: false
    showOnlyLastNames: true
  "git-plus":
    experimental:
      diffBranches: true
      useSplitDiff: false
    general:
      _analyticsUserId: "d8d69ea3-1eab-40f7-a718-ef4a9c1c9c98"
  "highlight-line":
    enableBackgroundColor: false
  "ide-java":
    errors:
      incompleteClasspathSeverity: "ignore"
  jshint:
    supportLintingJsx: true
  linter:
    ignoreGlob: "**/*.min.{js,css} **/*.jsx"
  "linter-tslint": {}
  "linter-ui-default":
    panelHeight: 177
  minimap: {}
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
    syncHorizontalScroll: true
  "sync-settings":
    _analyticsUserId: "ce5df772-4da0-4bb9-8f26-614728dc39fe"
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
".java.source":
  editor:
    preferredLineLength: 100
".source.ts":
  editor:
    scrollPastEnd: false
    showIndentGuide: false
