"*":
  "atom-beautify":
    csv:
      disabled: true
    general:
      _analyticsUserId: "795eef1c-c9a2-45a0-8c4a-e32d8da410cf"
    go:
      beautify_on_save: true
      disabled: true
    html:
      end_with_newline: true
      wrap_line_length: 70
    js:
      brace_style: "collapse-preserve-inline"
      default_beautifier: "Prettier"
      end_with_comma: true
      end_with_newline: true
      object_curly_spacing: true
    json:
      default_beautifier: "Pretty Diff"
      end_with_newline: true
    jsx:
      beautify_on_save: true
      brace_style: "collapse-preserve-inline"
      default_beautifier: "JS Beautify"
      e4x: false
      end_with_newline: true
      object_curly_spacing: true
      space_before_conditional: false
    python:
      max_line_length: 120
      multi_line_output: "Hanging Indent"
    scss:
      preserve_newlines: true
    typescript:
      break_chained_methods: true
      end_with_newline: true
      indent_size: 2
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnSave: true
    "atom-ide-debugger":
      showDebuggerOnBpSet: false
    hyperclick:
      darwinTriggerKeys: "altKey"
    use:
      "atom-ide-debugger": "never"
  "atom-jest-snippets": {}
  "atom-package-deps":
    ignored: [
      "linter-ui-default"
    ]
  "atom-shell-commands":
    commands: [
      {
        command: "echo moooooo"
        name: "SmartBuild"
        options:
          cwd: "{FileDir}"
          keymap: "cmd-shift-b"
      }
    ]
  "atom-typescript":
    autocompletionSuggestionPriority: 1
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    minimumWordLength: 1
    useCoreMovementCommands: false
  "autocomplete-snippets": {}
  autosave:
    enabled: true
  "configurable-autocomplete-snippets":
    suggestionPriority: 4
  core:
    allowPendingPaneItems: false
    audioBeep: false
    closeDeletedFileTabs: true
    closeEmptyWindows: false
    customFileTypes:
      "source.shell": [
        "envrc"
      ]
    disabledPackages: [
      "macros"
      "linter-eslint"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "README.md"
      "package-lock.json"
      "font-awesome.css"
      "google-fonts.css"
      "typeahead.css"
      "*.eot"
      "*.svg"
      "*.ttf"
      "*.woff"
      "*.woff2"
      "__snapshots__"
      "yarn.lock"
      "package.lock"
      "src/assets/charting_library/*"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "emmet"
      "multi-cursor"
      "goto-definition"
      "grammar-selector"
      "atom-sync"
      "toggle-quotes"
    ]
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "no"
    warnOnLargeFileLimit: 1
  "coverage-gutter":
    basePath: "/Users/elliotplant"
    pathToLCOV: "../../Users/elliotplant/Documents/dark-pool-web-client/dpwui/assets/coverage/lcov.info"
  "cursor-history": {}
  "disable-keybindings":
    prefixKeys: [
      "cmd-k"
    ]
  editor:
    fontSize: 12
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  "exception-reporting":
    userId: "c8be361d-65b0-e083-6643-808e2d26edb2"
  "file-icons":
    colourChangedOnly: true
  "file-types":
    $debug: true
    Dockerfile: {}
    "Dockerfile.*": "source.dockerfile"
  fonts: {}
  "fuzzy-finder":
    scoringSystem: "fast"
    useRipGrep: true
  "git-blame":
    columnWidth: 231
    dateFormatString: "MMM-DD"
    showHash: false
    showOnlyLastNames: true
  "git-plus":
    experimental:
      diffBranches: true
      useSplitDiff: false
    general:
      _analyticsUserId: "d8d69ea3-1eab-40f7-a718-ef4a9c1c9c98"
  "go-plus":
    autocomplete:
      suppressBuiltinAutocompleteProvider: false
    config: {}
    disableToolCheck: true
    lint:
      tool: "golangci-lint"
    navigator:
      mode: "guru"
    panel:
      displayMode: "closed"
      focusOnFailure: false
    test:
      runTestsOnSave: false
  "goto-definition":
    performanceMode: true
  "highlight-line": {}
  "ide-java":
    errors:
      incompleteClasspathSeverity: "ignore"
  "jest-test-toggler":
    projectRootPath: "/Users/elliotplant/Documents/lit-pool-web-client"
    specDefaultPath: "src"
    specSearchPaths: [
      "src"
    ]
  jshint:
    supportLintingJsx: true
  linter: {}
  "linter-eslint":
    autofix:
      fixOnSave: true
    disabling:
      disableWhenNoEslintConfig: false
    global:
      globalNodePath: "/usr/local"
    scopes: [
      "source.js"
      "source.jsx"
      "source.js.jsx"
      "source.flow"
      "source.babel"
      "source.js-semantic"
      "source.ts"
      "source.tsx"
    ]
  "linter-flake8":
    maxLineLength: 120
  "linter-tslint":
    fixOnSave: true
  "linter-ui-default":
    panelHeight: 157
    showProviderName: true
  "markdown-preview":
    breakOnSingleNewline: true
  minimap: {}
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
  "spell-check":
    addKnownWords: true
    excludedScopes: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain.null-grammar"
    ]
    knownWords: [
      "Binance"
      "collateralization"
      "Coinbase"
      "Bitstamp"
      "excersized"
      "hemheroid"
    ]
    locales: [
      "en-US"
    ]
    noticesMode: "console"
  "split-diff":
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
    syncHorizontalScroll: true
  "status-bar": {}
  "sync-settings":
    _analyticsUserId: "ce5df772-4da0-4bb9-8f26-614728dc39fe"
  "toggle-quotes":
    quoteCharacters: "\"'`"
  "tree-view":
    alwaysOpenExisting: true
  "vim-mode-plus":
    askOptInToEditorScrollPastEnd: false
    highlightSearch: false
    keymapPToPutWithAutoIndent: true
    keymapYToYankToLastCharacterOfLine: true
  welcome:
    showOnStartup: false
".diff.source":
  editor:
    softWrap: false
".java.source":
  editor:
    preferredLineLength: 100
".plain.text":
  whitespace:
    removeTrailingWhitespace: false
".source.ts":
  editor:
    scrollPastEnd: false
    showIndentGuide: false
